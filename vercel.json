{
    "rewrites": [
        { "source": "/(.*)", "destination": "/index.html" }
      ],
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ]
}